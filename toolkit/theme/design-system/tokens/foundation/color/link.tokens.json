              },
              "@focus": {
                "$type": "color",
                "$value": "{kda.foundation.color.semantic.positive.n80}"
              },
              "@visited": {
                "$type": "color",
                "$value": "{kda.foundation.color.palette.magenta.n80}"
              }
            },
            "negative": {
              "default": {
                "$type": "color",
                "$value": "{kda.foundation.color.semantic.negative.n90}"
              },
              "@hover": {
                "$type": "color",
