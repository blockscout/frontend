              },
              "@focus": {
                "$type": "color",
                "$value": "{kda.foundation.color.brand.primary.n60}"
              },
              "@visited": {
                "$type": "color",
                "$value": "{kda.foundation.color.palette.magenta.n80}"
              }
            },
            "secondary": {
              "default": {
                "$type": "color",
                "$value": "{kda.foundation.color.brand.secondary.n70}"
              },
              "@hover": {
                "$type": "color",
