              },
              "@focus": {
                "$type": "color",
                "$value": "{kda.foundation.color.brand.secondary.n60}"
              },
              "@visited": {
                "$type": "color",
                "$value": "{kda.foundation.color.palette.magenta.n80}"
              }
            }
          },
          "semantic": {
            "positive": {
              "default": {
                "$type": "color",
